	<script>
		"use strict";

		var changeFotoHeight = function() {
			var windowHeight = $(window).height();
			var containerMagnification = 0.75;
			var containerElem = $('.featured-foto-container');
			var fotoElem = $('.featured-foto-container img');
			var fotoElemHeight = fotoElem.height();
			var actualContainerHeight = containerElem.height();
			var newContainerHeight = Math.round(windowHeight * containerMagnification);
			containerElem.css('max-height', newContainerHeight);
			var imgMarginTop = fotoElemHeight > actualContainerHeight ? -1*Math.round((fotoElemHeight - actualContainerHeight)/2) : -1*Math.round((actualContainerHeight - fotoElemHeight)/2);
			fotoElem.css('margin-top', imgMarginTop);
		};

		$(document).ready(function() {
			initPhotoSwipeFromDOM('.featured-foto-container');
			changeFotoHeight();
			var ellipsisLines = 3;
			if (isMobile) {
				ellipsisLines = 2;
			}
			$('.post-title-homepage').ellipsis({ lines: ellipsisLines, responsive: true });
			setTimeout(changeFotoHeight, 50);
			setTimeout(changeFotoHeight, 250);
		});

		$(window).resize(function(){
			changeFotoHeight();
			setTimeout(changeFotoHeight, 50);
			setTimeout(changeFotoHeight, 250);
		});
	</script>
